.jm-popup{position:fixed;max-height:100%;overflow-y:auto;background:#fff}.jm-popup__close{position:absolute;top:10px;right:10px;color:#666;font-size:24px;-webkit-transform:rotate(-45deg);transform:rotate(-45deg)}.jm-popup--center{left:50%;top:50%;-webkit-transform:translate3d(-50%,-50%,0);transform:translate3d(-50%,-50%,0)}.jm-popup--left{top:0;bottom:0;left:0}.jm-popup--right{top:0;right:0;bottom:0}.jm-popup--top{top:0;left:0;right:0}.jm-popup--bottom{right:0;bottom:0;left:0}.jm-center-enter-active,.jm-center-leave-active{transition-property:opacity}.jm-center-enter,.jm-center-leave-to{opacity:0}.jm-bottom-enter-active,.jm-bottom-leave-active,.jm-left-enter-active,.jm-left-leave-active,.jm-right-enter-active,.jm-top-enter-active,.jm-top-leave-active{transition-property:-webkit-transform;transition-property:transform;transition-property:transform,-webkit-transform}.jm-top-enter,.jm-top-leave-to{-webkit-transform:translate3d(0,-100%,0);transform:translate3d(0,-100%,0)}.jm-bottom-enter,.jm-bottom-leave-to{-webkit-transform:translate3d(0,100%,0);transform:translate3d(0,100%,0)}.jm-left-enter,.jm-left-leave-to{-webkit-transform:translate3d(-100%,0,0);transform:translate3d(-100%,0,0)}.jm-right-enter,.jm-right-leave-to{-webkit-transform:translate3d(100%,0,0);transform:translate3d(100%,0,0)}.jm-zoom-in-enter-active,.jm-zoom-in-leave-active{transition-property:opacity,-webkit-transform;transition-property:opacity,transform;transition-property:opacity,transform,-webkit-transform;-webkit-transform-origin:center center;transform-origin:center center}.jm-zoom-in-enter,.jm-zoom-in-leave-to{opacity:0;-webkit-transform:translate3d(-50%,-50%,0) scale(.7);transform:translate3d(-50%,-50%,0) scale(.7)}