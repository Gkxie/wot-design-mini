<view
  jd:if="{{ inited }}"
  class="jm-transition custom-class {{ classes }}"
  style="-webkit-transition-duration: {{ currentDuration }}ms; transition-duration: {{ currentDuration }}ms; {{ display ? '' : 'display: none;' }} {{ customStyle }}"
  bind:transitionend="onTransitionEnd"
>
  <slot />
</view>