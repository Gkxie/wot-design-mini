.jm-notice-bar{display:-webkit-flex;display:flex;padding:5px 15px;-webkit-align-items:center;align-items:center;background:#fef8ef;color:#ff9000;font-size:12px}.jm-notice-bar__left-icon{min-width:30px;font-size:16px}.jm-notice-bar__right-icon{min-width:25px;text-align:right;font-size:10px}.jm-notice-bar__wrap{position:relative;-webkit-flex:1;flex:1;height:19px;overflow:hidden}.jm-notice-bar__content{position:absolute;white-space:nowrap;line-height:1.58}.jm-notice-bar__content--play{-webkit-animation:a linear both;animation:a linear both}.jm-notice-bar__content--play-infinite{-webkit-animation:a linear infinite both;animation:a linear infinite both}.jm-notice-bar--ellipse .jm-notice-bar__content{position:static;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.jm-notice-bar--wrap{padding:14px 15px}.jm-notice-bar--wrap .jm-notice-bar__wrap{height:auto}.jm-notice-bar--wrap .jm-notice-bar__content{position:static;white-space:normal}@-webkit-keyframes a{to{-webkit-transform:translate3d(-100%,0,0);transform:translate3d(-100%,0,0)}}@keyframes a{to{-webkit-transform:translate3d(-100%,0,0);transform:translate3d(-100%,0,0)}}