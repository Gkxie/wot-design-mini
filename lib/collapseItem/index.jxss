.jm-collapse-item__header{position:relative;display:-webkit-flex;display:flex;-webkit-justify-content:space-between;justify-content:space-between;-webkit-align-items:center;align-items:center;padding:11px 15px;overflow:hidden;-webkit-user-select:none;user-select:none}.jm-collapse-item__header:after{position:absolute;display:block;content:"";width:100%;height:1px;left:15px;top:0;-webkit-transform:scaleY(.5);transform:scaleY(.5);background:#eee}.jm-collapse-item__title{color:#333;font-size:16px}.jm-collapse-item__arrow{display:block;font-size:14px;color:#c5c5c5;transition:-webkit-transform .3s;transition:transform .3s;transition:transform .3s,-webkit-transform .3s}.jm-collapse-item__arrow.is-retract{-webkit-transform:rotate(-180deg);transform:rotate(-180deg)}.jm-collapse-item__wrapper{position:relative;transition:height .3s ease-in-out;overflow:hidden;will-change:height}.jm-collapse-item__wrapper:after{position:absolute;display:block;content:"";width:100%;height:1px;left:15px;top:0;-webkit-transform:scaleY(.5);transform:scaleY(.5);background:#eee}.jm-collapse-item__body{padding:13px 15px;color:#666;font-size:13px}.jm-collapse-item.is-disabled .jm-collapse-item__arrow,.jm-collapse-item.is-disabled .jm-collapse-item__title{color:#d2d2d2}.jm-collapse-item .jm-collapse-item__header-first:after{display:none}