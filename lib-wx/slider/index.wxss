.jm-slider{display:-webkit-flex;display:flex;-webkit-flex-flow:row nowrap;flex-flow:row nowrap;-webkit-align-items:center;align-items:center;height:42px}.jm-slider__label-cur,.jm-slider__label-max,.jm-slider__label-min{font-size:14px;color:#333}.jm-slider__label-cur{text-align:center;line-height:28px;background-color:hsla(0,0%,100%,.5);border-radius:100%}.jm-slider__axle{-webkit-flex-grow:1;flex-grow:1;position:relative;height:6px;border-radius:3px;background-color:#e5e5e5;margin:0 14px;z-index:0}.jm-slider__axle--has-min-max{margin:0 21px}.jm-slider__handle-container{position:absolute;top:-39px;left:-14px;display:-webkit-flex;display:flex;-webkit-flex-flow:column-reverse nowrap;flex-flow:column-reverse nowrap;height:56px;z-index:2}.jm-slider__handle{height:28px;width:28px;background-color:#fff;border-radius:100%;border:1px solid #e5e5e5;box-shadow:0 2px 4px 0 rgba(0,0,0,.3)}.jm-slider__progress-bar{height:6px;border-radius:3px;background-color:#0084ff;position:absolute;z-index:1}.jm-slider--has-label{padding-top:14px}.jm-slider--disabled .jm-slider__progress-bar{background-color:#e5f3ff}.jm-slider--disabled .jm-slider__label-max,.jm-slider--disabled .jm-slider__label-min{color:#d2d2d2}