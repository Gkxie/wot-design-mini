.jm-cell{padding-left:15px;background-color:#fff;text-decoration:none;color:#333;-webkit-tap-highlight-color:transparent}.jm-cell,.jm-cell__wrapper{position:relative;display:-webkit-flex;display:flex}.jm-cell__wrapper{-webkit-flex:1;flex:1;padding:12px 15px 12px 0;-webkit-justify-content:space-between;justify-content:space-between}.jm-cell__wrapper:after{position:absolute;display:block;content:"";width:100%;height:1px;left:0;top:0;-webkit-transform:scaleY(.5);transform:scaleY(.5);background:#eee}.jm-cell__title{font-size:16px;vertical-align:middle}.jm-cell__label{margin-top:3px;font-size:12px;color:#a7a7a7}.jm-cell__right{position:relative;font-size:0;text-align:right;vertical-align:middle}.jm-cell__icon{position:relative;min-width:18px;height:46px;line-height:46px;margin-right:15px;font-size:18px}.jm-cell__value{color:#666}.jm-cell__arrow-right,.jm-cell__value{display:inline-block;font-size:14px;vertical-align:middle}.jm-cell__arrow-right{margin-left:5px;height:22px;min-width:14px;line-height:22px;color:#c5c5c5}.jm-cell.is-no-hair .jm-cell__wrapper:after{display:none}.jm-cell.is-link{-webkit-tap-highlight-color:rgba(0,0,0,.06)}.jm-cell.is-hover{background-color:rgba(0,0,0,.06)}